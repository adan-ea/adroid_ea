{
    "name": "adroid_ea",
    "description": "Multi function discord bot",
    "version": "4.0.0",
    "author": "adan_ea",
    "license": "AGPL-3.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adroidea/bot.git"
    },
    "bugs": {
        "url": "https://github.com/adroidea/bot/issues",
        "email": "adan.ea+adroid@proton.me"
    },
    "keywords": [
        "discord",
        "bot",
        "adroid_ea",
        "typescript",
        "discord.js"
    ],
    "main": "index.js",
    "scripts": {
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "format": "prettier --write \"**/*.ts\"",
        "clean": "npm run lint:fix && npm run format",
        "watch": "tsc -w",
        "start": "tsc && node dist/index.js",
        "build": "tsc",
        "use:adroi.d.ea": "npm uninstall ..\\adroi.d.ea\\ && npm install adroi.d.ea",
        "use:adroi.d.ea:dist": "npm uninstall adroi.d.ea && npm install ..\\adroi.d.ea\\",
        "pre-commit": "lint-staged",
        "publish": "node .github/publish.js",
        "test": "jest --passWithNoTests",
        "dev": "npm-run-all --parallel typesafe-i18n start",
        "typesafe-i18n": "typesafe-i18n"
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ]
    },
    "dependencies": {
        "adroi.d.ea": "file:../adroi.d.ea",
        "ansis": "^1.5.5",
        "bullmq": "^3.12.0",
        "discord.js": "^14.14.1",
        "dotenv": "^16.0.3",
        "ioredis": "^5.3.2",
        "mongoose": "^7.5.0",
        "node-cron": "^3.0.2",
        "node-twitch": "^0.5.0",
        "typesafe-i18n": "^5.26.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.4",
        "@types/node": "^20.4.0",
        "@types/node-cron": "^3.0.7",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.3",
        "jest": "^29.6.4",
        "lint-staged": "^13.2.2",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.4",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.3"
    }
}
